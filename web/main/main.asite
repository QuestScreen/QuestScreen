<!doctype html>
<html lang="en">
<head>
	<title>QuestScreen Control Center</title>
	<link href="/pure-min.css" rel="stylesheet"/>
	<link href="/grids-responsive-min.css" rel="stylesheet"/>
	<link href="/fontawesome.min.css" rel="stylesheet">
	<link href="/solid.min.css" rel="stylesheet">
	<link href="/style.css" rel="stylesheet"/>
	<link href="/color.css" rel="stylesheet"/>

	<meta name="viewport" content="width=device-width, initial-scale=1"/>
	<meta charset="UTF-8"/>

	<!-- favicon stuff -->

	<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
	<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
	<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
	<link rel="manifest" href="/site.webmanifest">
	<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
	<meta name="msapplication-TileColor" content="#00aba9">
	<meta name="theme-color" content="#ffffff">
</head>
<body>
<a:site>
	<a:import>
		"github.com/QuestScreen/QuestScreen/web/controls"
	</a:import>
</a:site>
<a:embed name="Header" type="HeaderContainer"></a:embed>

<a:embed name="TitleContent" type="TitleBar"></a:embed>

<div id="page">
	<div id="pagemenu-wrapper">
		<nav id="pagemenu" class="pure-menu">
			<a:embed name="Menu" optional></a:embed>
		</nav>
	</div>

	<main>
		<a:embed name="Page" optional></a:embed>
	</main>

	<a:embed name="Popup" type="controls.PopupBase"></a:embed>
</body>
</html>